    var free3 = { items:[],sections:""};
    var adding = false;
    var cart1 =
        document.querySelector("cart-notification") ||
        document.querySelector("cart-drawer");
    function changeAddToCartText(type, step) {
	  if(step==1){
        if (type == "buy") {
			document.querySelector(".buyNowBtn").innerHTML = "Processing";
        }
        else if (type = "atc") {
            document.querySelector(".addToCartText").innerHTML = "Adding";
        }
	  }
	  else{
        if (type == "buy") {
			document.querySelector(".buyNowBtn").innerHTML = "Buy Now";
        }
        else if (type = "atc") {
		  setTimeout(()=>{

            document.querySelector(".addToCartText").innerHTML = "Add to Cart";
		  adding = false;
		  },800)

        }
	  }
    }
    function addAllToCart(parse) {
	  /*
	  if(parse=="atc"){
		document.querySelector("#labour-addToCart-btn").click();
	  }
	  else{
		document.querySelector("#labour-buyNow-btn").click();
	  }*/
        if (adding == false) {
            changeAddToCartText(parse,1);
            adding = true;
            var form = document.querySelector("form#product_form_7523710664952");
            let formData = new FormData(form);
            id = formData.get("id");
            free3.items.unshift({ id: id, quantity: 1 });
            var sections = cart1.getSectionsToRender().map((section) => section.id);
            free3.sections = sections.join(",");
            fetch("/cart/add.js", {
                method: "POST",
                body: JSON.stringify(free3),
                headers: {
                    "Content-Type": "application/json",
                },
            })
                .then((res) => res.json())
                .then((res1) => {
                    if (parse == "buy") {
                        window.location.href = "/cart"
                    }
                    else {
                        res1.key = "";
                        cart1.renderContents(res1);
                        changeAddToCartText(parse, 0);
                        free3.items.splice(0, 1);
                    }
                })
                .catch((err) => {
                    changeAddToCartText(parse, 0);
			  		$(".error-tip").css("visibility","visible");
					setTimeout(()=>{
						  $(".error-tip").css("visibility","hidden");
					},5000)
                });
        }
    }
</script>
