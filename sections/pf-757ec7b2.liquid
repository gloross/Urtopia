<script>
function nextSwiper() {
  let current_i = +document
    .querySelector("#mobile-swiper .left_img_container .left_img[data-active]")
    .getAttribute("data-index");
  if (current_i >= 2) return;

  document
    .querySelectorAll("#mobile-swiper .left_img_container .left_img")
    .forEach((v) => v.removeAttribute("data-active"));
  document
    .querySelectorAll("#mobile-swiper .left_img_container .left_img")
    [+current_i + 1].setAttribute("data-active", true);
  let swiperLeftItem = document.querySelectorAll(
    "#mobile-swiper .left_img_container .left_img"
  );
  let swiperRightItem = document.querySelectorAll(
    "#mobile-swiper .right_des_container .right_des"
  );

  TweenMax.staggerTo(
    [swiperLeftItem[current_i], swiperRightItem[current_i]],
    0.7,
    {
      x: "150%",
      opacity: 0,
      onComplete: () => {
        TweenMax.staggerTo(
          ".mobile-swiper-container .swiper_item .left_img img.active_img",
          0.5,
          {
            opacity: 0,
            top: "50%",
            left: "50%",
            x: "-50%",
            y: "-50%",
          }
        );
      },
    }
  );
  TweenMax.staggerTo(
    [swiperLeftItem[current_i + 1], swiperRightItem[current_i + 1]],
    0.7,
    {
      x: "0%",
      opacity: 1,
      onComplete: () => {
        TweenMax.to(
          ".mobile-swiper-container .left_img:nth-child(" +
            (current_i + 2) +
            ") img.active_img:nth-child(2)",
          0.5,
          {
            opacity: 1,
            top: "50%",
            left: "50%",
            y: "-50%",
            x: "-130%",
