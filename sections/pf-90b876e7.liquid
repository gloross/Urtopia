			break;
		  case 2:
			des.html("“Leicht, vernetzt und sehr sicher„");
			img.attr("src","https://cdn.shopify.com/s/files/1/0633/2068/6808/t/4/assets/1200pxntv_logoseptember2017svg2x-1672134438688.png")
			break;
		  case 3:
			des.html("“Das beste Elektrofahrrad für 2022„");
			img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/cnet-1671527740909.png")
			break;
		  case 4:
			des.html("“Eines der intelligentesten und leichtesten E-Bikes„");
			img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/img_v2_1f5341a218124ab39b4bb5cd49cc335g-1668500914564.png")
			break;
		  case 5:
			des.html("“Bessere Telefonie als einige Smartphones„");
			img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/img_v2_c977d9efae3a4c46ad2ddac55c5dabfg-1668500914568.png")
			break;
		  default:
		}
		setTimeout(() => {
		  container.css("transition", "all 0.5s");
		  container.css("opacity", "1");
		  container.css("transform", "translateY(0)");
