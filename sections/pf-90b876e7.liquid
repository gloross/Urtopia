<script>
  $(window).load(function () {
	chekoutInventory();
  });
  $("#urtopiaEbike input").change(function () {
	setTimeout(() => {
	  chekoutInventory();
	}, 100);
  });

  function chekoutInventory() {
	let formData = new FormData(
	  document.querySelector("form#product_form_7633738727640")
	);
	let productQuantity = {};
	let styleName = "";
	for (const key of formData.keys()) {
	  if (key.indexOf("Style") != -1) {
		styleName = key;
	  }
	}

	products["7633738727640"].quantity.forEach((item) => {
	  let temp = item.split(":");
	  productQuantity[temp[0]] = temp[1];
	});

	if (formData.get(styleName) === "vanilla") {
	  $(".tip-b").html(
		"This is a back-ordered item to be shipped on <span>Feb 15-28, 2023</span>."
	  );
	} else {
	  $(".tip-b").html(
		"Ships within <span>3 business days</span> from local warehouse."
	  );
	}

	if (productQuantity[formData.get("id")] <= 0) {
	  $(".addToCartBtn")[0].disabled = true;
	  $(".addToCartBtn span").html("Nicht auf Lager");
	  $(".buyNowBtn").addClass("soldout");
	} else {
	  $(".addToCartBtn")[0].disabled = false;
	  $(".addToCartBtn span").html("Add to Cart");
	  $(".buyNowBtn").removeClass("soldout");
	}
  }
  $(".btn span").click(function () {
	$(".pay-content-bottom").toggleClass("down");
	$(".btn span").toggleClass("down");
  });
  window.addEventListener("scroll", () => {
	let target = document.querySelector("#pay-content-container");
	let showTop = document
	  .querySelector("#shippment")
	  .getBoundingClientRect().top;

	if (showTop <= 1) {
	  //target.style.transform = "translateY(0)";
	  $("#pay-content-container").addClass("active");
	  $("#shopify-section-header").addClass("active");
	} else {
	  //target.style.transform = "translateY(-100%)";
	  $("#pay-content-container").removeClass("active");
	  $("#shopify-section-header").removeClass("active");
	}
  });
  function trace() {
	let body = {
	  trace_name: "de-order-buy",
	  extras: {},
	};
	fetch("https://api.newurtopia.com/third_part/book_ride/traces", {
	  method: "POST",
	  body: JSON.stringify(body),
	  headers: {
		"Content-Type": "application/json",
	  },
	});
  }

</script>
